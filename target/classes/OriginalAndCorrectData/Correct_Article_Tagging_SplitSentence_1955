a#V=VEfin#estar enamorado#V=VLadj#enamorar de#Prep=PREP#de la#Det=ART#el tele#N=NC#tele y#Conj=CC#y le#Pron=PPC#Ã©l|le daba#V=VLfin#dar muchos#Adj=QU#mucho besos#N=NC#beso ,#null#, Cupido#N=NP#Cupido se#Pron=SE#se había#V=VLfin#había equivocado#V=VLadj#equivocar y#Conj=CC#y Estela#N=NP#Estela estaba#V=VEfin#estar muy#Adv=ADV#muy disgustada#V=VLadj#disgustar y#Conj=CC#y lloró#V=VLfin#lloró desesperadamente#Adv=ADV#desesperado .#Los_demás=FS#.$$