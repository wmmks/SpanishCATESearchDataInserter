eta#Adj=ADJ#coqueta .#Los_demás=FS#.$Javier#N=NP#Javier es#V=VSfin#ser tímido#Adj=ADJ#tímido .#Los_demás=FS#.$A#Prep=PREP#a Estela#N=NP#Estela le#Pron=PPC#Ã©l|le gusta#V=VLfin#gustar Javier#N=NP#Javier ,#null#, ella#Pron=PPX#Ã©l no#Adv=NEG#no puede#V=VMfin#poder vivir#V=VLinf#vivir sin#Prep=PREP#sin él#N=NC#él .#Los_demás=FS#.$Cupido#N=NP#Cupido es#V=VSfin#ser travieso#V=VLfin#travesar y#Conj=CC#y está#V=VLfin#está indeciso#Adj=ADJ#indeciso en#Prep=PREP#en unir#V=VLinf#unir a#Prep=PREP#a Javier#N=NP#Javier y#Conj=CC#y a#Prep=PREP#a Estela#N=NP#Estela .#Los_demás=FS#.$El#Det=ART#el programa#N=NC#programa de#Prep=PREP#de televisión#N=NC#televisión es#V=VSfin#ser entretenido#Adj=ADJ#entretenido ,#null#, es#V=VSfin#ser el#Det=ART#el programa#N=NC#programa favorito#Adj=ADJ#favorito de#Prep=PREP#de Javier#N=NP#Javier .#Los_demás=FS#.$Cupido#N=NP#Cupido lanza#V=VLfin#lanzar una#Det=ART#un flecha#N=NC#flecha .#Los_demás=FS#.$Todos#Adj=QU#todo están#N=NC#están sorprendidos#V=VLadj#sorprender .#Los_demás=FS#.$Javier#N=NP#Javier se#Pron=SE#se ha#V=VHfin#haber enamorado#V=VLadj#enamorar de#Prep=PREP#de la#Det=ART#el televisión#N=NC#televisión .#Los_demás=FS#.$Estela#N=NP#Estela está#V=VLfin#está disgustada#V=VLadj#disgustar .#Los_demás=FS#.$$