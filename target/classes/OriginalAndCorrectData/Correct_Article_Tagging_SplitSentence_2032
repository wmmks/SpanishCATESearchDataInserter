eta .#Los_demás=FS#.$Cupido#N=NP#Cupido es#V=VSfin#ser travieso#V=VLfin#travesar y#Conj=CC#y está#V=VLfin#está indeciso#Adj=ADJ#indeciso en#Prep=PREP#en unir#V=VLinf#unir a#Prep=PREP#a Javier#N=NP#Javier y#Conj=CC#y a#Prep=PREP#a Estela#N=NP#Estela .#Los_demás=FS#.$Javier#N=NP#Javier está#V=VLfin#está nervioso#Adj=ADJ#nervioso ,#null#, pero#Conj=CCAD#pero Estela#N=NP#Estela está#N=NC#está interesada#Adj=ADJ#interesado .#Los_demás=FS#.$Javier#N=NP#Javier mira#V=VLfin#mirar su#Pron=PPO#suyo programa#N=NC#programa de#Prep=PREP#de televisión#N=NC#televisión favorito#Adj=ADJ#favorito .#Los_demás=FS#.$El#Det=ART#el programa#N=NC#programa es#V=VSfin#ser entretenido#Adj=ADJ#entretenido .#Los_demás=FS#.$Cupido#N=NP#Cupido lanza#V=VLfin#lanzar una#Det=ART#un flecha#N=NC#flecha a#Prep=PREP#a Javier#N=NP#Javier .#Los_demás=FS#.$Estela#N=NP#Estela está#V=VLfin#está ilusionada#V=VLadj#ilusionar .#Los_demás=FS#.$El#Det=ART#el resultado#N=NC#resultado es#V=VSfin#ser sorprendente#Adj=ADJ#sorprendente ,#null#, porque#Conj=CSUBX#porque Javier#N=NP#Javier se#Pron=SE#se ha#V=VHfin#haber enamorado#V=VLadj#enamorar de#Prep=PREP#de la#Det=ART#el niña#N=NC#niña del#Los_demás=PDEL#del programa#N=NC#programa .#Los_demás=FS#.$Estela#N=NP#Estela está#V=VLfin#está disgustada#V=VLadj#disgustar .#Los_demás=FS#.$$