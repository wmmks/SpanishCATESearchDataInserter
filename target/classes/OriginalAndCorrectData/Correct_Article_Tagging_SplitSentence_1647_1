iente se#Pron=SE#se ha#V=VHfin#haber vuelto#V=VLadj#volver muy#Adv=ADV#muy tranquilo#Adj=ADJ#tranquilo sin#Prep=PREP#sin los#Det=ART#el hijos#N=NC#hijo .#Los_demás=FS#.$Los#Det=ART#el padres#N=NC#padre creen#V=VLfin#crear que#Conj=CQUE#que los#Det=ART#el hijos#N=NC#hijo también#Adv=ADV#también están#N=NC#están nostálgicos#Adj=ADJ#nostálgicos .#Los_demás=FS#.$Después#Adv=ADV#Después de#Prep=PREP#de cuatro#Adj=CARD#cuatro años#N=NC#años ,#null#, los#Det=ART#el hijos#N=NC#hijo vuelven#V=VLfin#volver a#Prep=PREP#a casa#N=NC#casa .#Los_demás=FS#.$Los#Det=ART#el padres#N=NC#padre están#Adj=ADJ#están sorprendidos#V=VLadj#sorprender ,#null#, porque#Conj=CSUBX#porque los#Det=ART#el hijos#N=NC#hijo han#V=VHfin#haber cambiado#V=VLadj#cambiar mucho#Adv=ADV#mucho .#Los_demás=FS#.$Su#Pron=PPO#suyo ropa#N=NC#ropa es#V=VSfin#ser extravagante#Adj=ADJ#extravagante .#Los_demás=FS#.$Carlos#N=NP#Carlos tiene#V=VLfin#tener barba#N=NC#barba y#Conj=CC#y María#N=NP#María tiene#V=VLfin#tener el#Det=ART#el pelo#N=NC#pelo rizado#V=VLadj#rizar .#Los_demás=FS#.$Aun#Adv=ADV#aun así#V=VLfin#así ,#null#, los#Det=ART#el padres#N=NC#padre les#Pron=PPC#Ã©l dan#V=VLfin#dar la#Det=ART#el bienvenida#N=NC#bienvenida .#Los_demás=FS#.$$