ido .#Los_demás=FS#.$Cupido#N=NP#Cupido quiere#V=VLfin#querer unir#V=VLinf#unir a#Prep=PREP#a Javier#N=NP#Javier y#Conj=CC#y Estela#N=NP#Estela pero#Conj=CCAD#pero está#V=VLfin#está indeciso#Adj=ADJ#indeciso porque#Conj=CSUBX#porque aunque#Conj=CSUBX#aunque Estela#N=NP#Estela está#N=NC#está interesada#Adj=ADJ#interesado ,#null#, Javier#N=NP#Javier se#Pron=SE#se pone#V=VLfin#poner muy#Adv=ADV#muy nervioso#Adj=ADJ#nervioso .#Los_demás=FS#.$Javier#N=NP#Javier mira#V=VLfin#mirar su#Pron=PPO#suyo programa#N=NC#programa entretenido#Adj=ADJ#entretenido ,#null#, es#V=VSfin#ser su#Pron=PPO#suyo favorito#Adj=ADJ#favorito .#Los_demás=FS#.$Estela#N=NP#Estela está#V=VLfin#está ilusionada#V=VLadj#ilusionar por#Prep=PREP#por el#Det=ART#el programa#N=NC#programa y#Conj=CC#y también#Adv=ADV#también por#Prep=PREP#por Javier#N=NP#Javier .#Los_demás=FS#.$Cupido#V=VLfin#Cupido ya#Adv=ADV#ya decide#V=VLfin#decidir a#Prep=PREP#a lanzar#V=VLinf#lanzar una#Det=ART#un flecha#N=NC#flecha .#Los_demás=FS#.$El#Det=ART#el resultado#N=NC#resultado es#V=VSfin#ser muy#Adv=ADV#muy sorprendente#Adj=ADJ#sorprendente y#Conj=CC#y exagerado#Adj=ADJ#exagerado .#Los_demás=FS#.$Javier#N=NP#Javier está#V=VLfin#está enamorado#V=VLadj#enamorar con#Prep=PREP#con la#Det=ART#el tele#N=NC#tele .#Los_demás=FS#.$¡#V=VLfin#¡ Madre#N=NC#madre Mía#N=NP#Mía !#Los_demás=FS#!$Estela#N=NP#Estela no#Adv=NEG#no puede#V=VMfin#poder sorporta#V=VLfin#sorporta este#Pron=DM#este resultado#N=NC#resultado ,#null#, y#Conj=CC#y se#Pron=SE#se queda#V=VLfin#quedar muy#Adv=ADV#muy disgustada#V=VLadj#disgustar .#Los_demás=FS#.$$